<h1>User#create</h1>
<%= form_with url: "create", method: :post do |form| %>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= form.label :email, "Email" %>
  <%= form.text_field :email %>
  <%= form.label :password, "Password" %>
  <%= form.password_field :password %>
  <%= form.submit "Sign up" %>
<% end %>
