<h1>Deck#create</h1>
<p>Find me in app/views/deck/create.html.erb</p>
<%= form_with url: "../deck/create", method: :post do |form|%>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= form.label :Deck_name, "Deck" %>
  <%= form.text_field :Deck_name %>
  <%= form.label :Deck_description, "Deck Description" %>
  <%= form.text_field :Deck_description %>

  <%= form.submit %>
<% end %>

